    <script type="text/javascript">
        // based on prepared DOM, initialize echarts instance
        var myChart = echarts.init(document.getElementById("water_monthly"));
  
        // specify chart configuration item and data
        option = {
    title: {
        text: '',
        subtext: 'งบประมาณ (บาท)'
    },
    tooltip: {
        trigger: 'axis'
    },
    legend: {
        data: ['แผนเบิก', 'ผลใช้']
    },
    toolbox: {
        show: true,
        feature: {
            dataZoom: {
                yAxisIndex: 'none'
            },
            dataView: {readOnly: false},
            magicType: {type: ['line', 'bar']},
            restore: {},
            saveAsImage: {}
        }
    },
    xAxis: {
        type: 'category',
        boundaryGap: false,
        data: ['มกราคม', 'กุมภาพันธ์', 'มีนาคม', 'เมษายน', 'พฤษถาคม', 'มิถุนายน', 'กรกฏาคม', 'สิงหาคม', 'กันยายน', 'ตุลาคม','พฤษจิกายน','ธันวาคม',]
    },
    yAxis: {
        type: 'value',
        axisLabel: {
            formatter: '{value}'
        }
    },
    series: [
        {
            name: 'แผนเบิก',
            type: 'line',
            data: [1000000, 1000000,1000000,1000000,1000000,1000000,1000000,1000000,1000000,1000000,1000000,1000000],
            markPoint: {
                data: [
                    {type: 'max', name: 'ค่าสูงสุด'},
                    {type: 'min', name: 'ค่าต่ำสุด'}
                ]
            },
            markLine: {
                data: [
                    {type: 'average', name: '平均值'}
                ]
            }
        },
        {
            name: 'ผลใช้',
            type: 'line',
            data: [900000,950000,1050000,1000000,900000,950000,1050000,1000000,900000,950000,1050000,1000000],
            markPoint: {
                data: [
                    {name: '周最低', value: -2, xAxis: 1, yAxis: -1.5}
                ]
            },
            markLine: {
                data: [
                    {type: 'average', name: 'ค่าเฉลี่ย'},
                    [{
                        symbol: 'none',
                        x: '90%',
                        yAxis: 'max'
                    }, {
                        symbol: 'circle',
                        label: {
                            position: 'start',
                            formatter: 'ค่าสูงสุด'
                        },
                        type: 'max',
                        name: 'ค่าสูงุสด'
                    }]
                ]
            }
        }
    ]
};

  
        // use configuration item and data specified to show chart
        myChart.setOption(option);

      </script>
    
