        // specify chart configuration item and data
        option = {
    title: {
        text: '',
        subtext: 'งบประมาณ (บาท)'
    },
    tooltip: {
        trigger: 'axis'
    },
    legend: {
        data: ['แผนเบิก', 'ผลใช้']
    },
    toolbox: {
        show: true,
        feature: {
            dataZoom: {
                yAxisIndex: 'none'
            },
            dataView: {readOnly: false},
            magicType: {type: ['line', 'bar']},
            restore: {},
            saveAsImage: {}
        }
    },
    xAxis: {
        type: 'category',
        boundaryGap: false,
        data: ['มกราคม', 'กุมภาพันธ์', 'มีนาคม', 'เมษายน', 'พฤษถาคม', 'มิถุนายน', 'กรกฏาคม', 'สิงหาคม', 'กันยายน', 'ตุลาคม','พฤษจิกายน','ธันวาคม',]
    },
    yAxis: {
        type: 'value',
        axisLabel: {
            formatter: '{value}'
        }
    },
    series: [
        {
            name: 'แผนเบิก',
            type: 'line',
            data: [1000000, 1000000,1000000,1000000,1000000,1000000,1000000,1000000,1000000,1000000,1000000,1000000],
            markPoint: {
                data: [
                    {type: 'max', name: 'ค่าสูงสุด'},
                    {type: 'min', name: 'ค่าต่ำสุด'}
                ]
            },
            markLine: {
